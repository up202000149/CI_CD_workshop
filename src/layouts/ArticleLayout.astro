---
import Layout from "./Layout.astro";
import StyledMarkdown from "../components/StyledMarkdown.astro";

type Props = {
    frontmatter: {
        title: string;
    }
};

const { frontmatter } = Astro.props;

---

<Layout title={frontmatter.title}>
    <StyledMarkdown>
        <slot />
    </StyledMarkdown>
</Layout>
